datasource db {
    provider = "mongodb"
    url = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client.js"
}

model Example {
    id String @id @default(auto()) @map("_id") @db.ObjectId
    name String @unique
    public Boolean @default(false)
    userId String @db.ObjectId
    user User @relation(fields: [userId], references: [id])
}

model Post {
  id     String @id @default(auto()) @map("_id") @db.ObjectId
  title  String
  userId String @db.ObjectId
  user   User   @relation(fields: [userId], references: [id])
}

model User {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  email String
  post Post[]
  example Example[]
}
